<header id="header">
    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
                <div class="navbar-brand"><span>{{ page_name }}</span></div>
            </div>

            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    {% for key, val in admin_links %}
                        {% if admin_sublinks[key] %}
                            <li class="dropdown dropdown-special">
                                {% if '#' == val.url %}
                                    <a href="{{ val.url }}" id="admin_menu_{{ key }}_link}" class="dropdown-toggle" data-toggle="dropdown" title="{{ val.anchor }}">
                                        {{ val.anchor }} <span class="caret"></span>
                                    </a>
                                {% else %}
                                    <a href="{{ val.url }}" id="admin_menu_{{ key }}_link}" title="{{ val.anchor }}">
                                        {{ val.anchor }}
                                    </a>
                                    <span class="dropdown-caret dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></span>
                                {% endif %}
                                <ul class="dropdown-menu">
                                    {% for key2, val2 in admin_sublinks[key] %}
                                        <li><a href="{{ val2.url }}" title="{{ val2.anchor }}" id="admin_menu_{{ key2 }}_link2}">{{ val2.anchor }}</a></li>
                                    {% endfor %}
                                </ul>
                            </li>
                        {% else %}
                            <li><a href="{{ val.url }}" title="{{ val.anchor }}" id="admin_menu_{{ key }}_link}">{{ val.anchor }}</a></li>
                        {% endif %}
                    {% endfor %}
                </ul>
            </div>
        </div>
    </nav>
</header>